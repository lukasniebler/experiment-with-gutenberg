{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rrze-elements/accordion",
  "version": "1.25.10",
  "title": "Accordion",
  "category": "design",
  "icon": "align-center",
  "description": "Creates an accordion block",
  "attributes": {
    "message": {
      "type": "string",
      "source": "text",
      "selector": "div"
    },
    "expandAllLink": {
      "type": "boolean",
      "default": false
    },
    "hstart": {
      "type": "integer",
      "default": 2
    },
    "register": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "rrze-elements",
  "editorScript": "file:./index.js",
  "script": "rrze-accordions",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}